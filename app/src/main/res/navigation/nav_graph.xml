<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/nav_graph"
    app:startDestination="@id/mainFragment">

    <fragment
        android:id="@+id/mainFragment"
        android:name="dev.bulean.notwallet.ui.main.MainFragment"
        android:label="fragment_main"
        tools:layout="@layout/fragment_main">
        <action
            android:id="@+id/action_main_to_detail_asset"
            app:destination="@id/detailAssetFragment"
            app:enterAnim="@anim/nav_default_enter_anim"
            app:exitAnim="@anim/nav_default_exit_anim"
            app:popEnterAnim="@anim/nav_default_pop_enter_anim"
            app:popExitAnim="@anim/nav_default_pop_exit_anim" />
    </fragment>
    <fragment
        android:id="@+id/detailAssetFragment"
        android:name="dev.bulean.notwallet.ui.detail_asset.DetailAssetFragment"
        android:label="fragment_detail_asset"
        tools:layout="@layout/fragment_detail_asset">
        <argument
            android:name="assetName"
            app:argType="string"
            android:defaultValue="" />
    </fragment>
</navigation>
